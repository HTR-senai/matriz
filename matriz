#include <stdio.h>

int main()
{
    int i, j;
    int matriz[2][3] = {
        {1,2,3},
        {4,5,6}
        }; 
        
        
        printf("A matriz formatada: \n");
        
        for (i = 0; i < 2; i++){
            for (j = 0; j < 3; j++){
                printf("[%d][%d]= %d ", i, j, matriz[i][j]);
            }
            printf("\n");
        }
         
         return 0;
        }
